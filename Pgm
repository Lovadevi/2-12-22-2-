def vote(age):
   if age>=18:
      print("Yes")
   else:
      print("No")
age=int(input())
vote(age)



def vote(age):
   if age>=18:
      return "Yes"
   else:
      return "No"
age=int(input())
print(vote(age))



def vote(age):
   if age>=18:
      return True
   else:
      return False
age=int(input())
if vote(age):
   print("Yes")
else:
   print("No")
